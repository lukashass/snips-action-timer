{
  "name": "snips-action-timer",
  "version": "1.0.0",
  "description": "Snips action code for the Timer app",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run lint && npm run build && npm run test && npm run test:sandbox",
    "build": "snips-toolkit build",
    "dev": "snips-toolkit dev -c hermes-config.json",
    "test": "snips-toolkit test '^.*\\.spec\\.[jt]s$'",
    "test:sandbox": "snips-toolkit test -s '^.*\\.spec\\.[jt]s$'",
    "launch": "snips-toolkit run -c hermes-config.json",
    "lint": "eslint \"**/*.[jt]s\""
  },
  "author": "Julien Elbaz",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "eslint": "^5.15.3",
    "eslint-plugin-jest": "^22.4.1",
    "hermes-javascript": "^0.4.1",
    "typescript": "^3.3.4000",
    "snips-toolkit": "^1.0.0"
  },
  "dependencies": {
    "dayjs": "^1.8.12",
    "uuid": "^3.3.2",
    "snips-toolkit-runner": "^1.0.0"
  },
  "sandbox": [
    "crypto",
    "fs",
    "os",
    "path",
    "tty",
    "util"
  ]
}